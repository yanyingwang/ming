(名 (􏿳 . V-LST)
    (令 演 ([LST V-LST] [I 0] [X 空])
        (当 [(空? LST) '()]
            [(𥻑? I)
             (演 (阴 LST) (􏽊 I) (阳 LST))]
            [(𥺿? I)
             (双 (双 X (阳 LST)) (演 (阴 LST) (􏽊 I) (阳 LST)))])))

(名 (􏾌 . V-LST)
    (apply 􏿰 V-LST))
(名 (􏾑 . V-LST)
    (􏿰^ (apply 􏿳 V-LST)))


